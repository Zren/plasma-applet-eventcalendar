<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0 http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
	<kcfgfile name=""/>


	<group name="General">
		<entry name="debugging" type="bool">
			<default>false</default>
		</entry>
		<!-- Removed in: v33
		<entry name="widget_show_spacer" type="bool">
			<default>false</default>
		</entry>
		-->
		<!--
		<entry name="widget_show_pin" type="bool">
			<default>false</default>
		</entry>
		-->
		<entry name="pin" type="Bool">
			<label>Whether the popup should remain open when another window is activated</label>
			<default>false</default>
		</entry>
		<entry name="widgetShowMeteogram" type="bool">
			<default>true</default>
		</entry>
		<entry name="widgetShowTimer" type="bool">
			<default>true</default>
		</entry>
		<entry name="widgetShowAgenda" type="bool">
			<default>true</default>
		</entry>
		<entry name="widgetShowCalendar" type="bool">
			<default>true</default>
		</entry>
		<entry name="timerSfxEnabled" type="bool">
			<default>true</default>
		</entry>
		<entry name="timerSfxFilepath" type="string">
			<default>/usr/share/sounds/freedesktop/stereo/complete.oga</default>
		</entry>
		<entry name="clockFontFamily" type="string">
			<default></default>
		</entry>
		<entry name="clockTimeFormat" type="string">
			<!-- For Testing:  '&lt;font color="#888"&gt;'ddd'&lt;&gt;'d'&lt;/font&gt;' h:m:s'&lt;'AP'&gt;' -->
			<default></default>
		</entry>
		<entry name="clockTimeFormat2" type="string">
			<default></default>
		</entry>
                <entry name="clockLine2" type="bool">
			<default>false</default>
		</entry>
                <entry name="clockLine2HeightRatio" type="double">
			<default>0.4</default>
		</entry>
                <entry name="clockLine1Bold" type="bool">
			<default>false</default>
		</entry>
                <entry name="clockLine2Bold" type="bool">
			<default>false</default>
		</entry>
                <entry name="clockMaxHeight" type="int">
			<default>0</default>
		</entry>
                <entry name="clockMouseWheel" type="string">
			<default>run_commands</default>
		</entry>
                <entry name="clockMouseWheelUp" type="string">
			<default>qdbus org.kde.kglobalaccel /component/kmix invokeShortcut "increase_volume"</default>
		</entry>
                <entry name="clockMouseWheelDown" type="string">
			<default>qdbus org.kde.kglobalaccel /component/kmix invokeShortcut "decrease_volume"</default>
		</entry>
                <entry name="timerRepeats" type="bool">
			<default>false</default>
		</entry>
                <entry name="timerInTaskbar" type="bool"> <!-- Doesn't go anywhere, not used -->
			<default>false</default>
		</entry>
                <entry name="timerEndsAt" type="UInt"> <!-- Doesn't go anywhere, not used -->
			<default>0</default>
		</entry>
                <entry name="showOutlines" type="bool">
			<default>true</default>
		</entry>
		<entry name="showBackground" type="bool">
			<default>true</default>
		</entry>
		<entry name="topRowHeight" type="uint">
			<default>100</default>
		</entry>
		<entry name="bottomRowHeight" type="uint">
			<default>400</default>
		</entry>
		<entry name="leftColumnWidth" type="uint">
			<default>400</default>
		</entry>
		<entry name="rightColumnWidth" type="uint">
			<default>400</default>
		</entry>
	</group>

	<group name="Tooltip">
		<entry name="selectedTimeZones" type="StringList">
			<label>A list of the time zones available on mouse wheel. Requires wheelChangesTimezone to be true. Format is "Europe/London". Special entry "Local" indicates system time zone.</label>
			<default>Local</default>
		</entry>
		<entry name="displayTimezoneAsCode" type="Bool">
			<label>Whether the timezone is displayed as a code i.e. "GMT" or full text i.e. "London".</label>
			<default>true</default>
		</entry>
	</group>
	
	<group name="Calendar">
                <entry name="monthShowBorder" type="bool">
			<default>true</default>
		</entry>
                <entry name="monthShowWeekNumbers" type="bool">
			<default>false</default>
		</entry>
		<entry name="monthDayDoubleClick" type="string">
			<default>GoogleCalWeb</default>
		</entry>
                <entry name="monthEventBadgeType" type="string">
			<default>theme</default>
		</entry>
                <entry name="monthTodayStyle" type="string">
			<default>theme</default>
		</entry>
                <entry name="monthCellRadius" type="double">
			<default>0</default>
		</entry>
		<entry name="firstDayOfWeek" type="int">
			<!-- Enum: -1 = Locale default, 0=Sunday, 1=Monday, ..., 6=Saturday -->
			<default>-1</default>
		</entry>
		<entry name="monthHighlightCurrentDayWeek" type="bool">
			<default>true</default>
		</entry>
		<entry name="monthHeightSingleColumn" type="uint">
			<default>300</default>
		</entry>
	</group>

	<group name="Agenda">
		<entry name="twoColumns" type="bool">
			<default>true</default>
		</entry>
		<entry name="agendaWeatherOnRight" type="bool">
			<default>false</default>
		</entry>
                <entry name="agendaWeatherShowIcon" type="bool">
			<default>true</default>
		</entry>
                <entry name="agendaWeatherIconHeight" type="UInt">
			<default>24</default>
		</entry>
                <entry name="agendaWeatherShowText" type="bool">
			<default>false</default>
		</entry>
                <entry name="agendaBreakupMultidayEvents" type="bool">
			<default>false</default>
		</entry>
                <entry name="agendaNewEventRememberCalendar" type="bool">
			<default>true</default>
		</entry>
                <entry name="agendaNewEventLastCalendarId" type="string">
			<default></default>
		</entry>
                <entry name="agendaInProgressColor" type="string" stringType="color">
			<default></default>
		</entry>
                <entry name="agendaFontSize" type="uint">
			<default>0</default>
		</entry>
		<entry name="agendaDaySpacing" type="uint">
			<default>20</default>
		</entry>
		<entry name="agendaEventSpacing" type="uint">
			<default>10</default>
		</entry>
		<entry name="agendaShowEventDescription" type="bool">
			<default>true</default>
		</entry>
		<entry name="agendaShowEventHangoutLink" type="bool">
			<default>true</default>
		</entry>
		<entry name="agendaCondensedAllDayEvent" type="bool">
			<default>true</default>
		</entry>
		<entry name="agendaPlaceOverdueTasksOnToday" type="bool">
			<default>true</default>
		</entry>
	</group>


	<group name="Events">
		<entry name="icalCalendarList" type="string" stringType="base64json">
			<default></default>
		</entry>
		<entry name="eventStartingNotificationEnabled" type="bool">
			<default>true</default>
		</entry>
		<entry name="eventStartingSfxEnabled" type="bool">
			<default>true</default>
		</entry>
		<entry name="eventStartingSfxPath" type="string">
			<default>/usr/share/sounds/Oxygen-Im-Nudge.ogg</default>
		</entry>
		<entry name="enabledCalendarPlugins" type="StringList">
			<label>A list of plugins where additional calendar event data can be sourced.</label>
			<default>/usr/lib/x86_64-linux-gnu/qt5/plugins/plasmacalendarplugins/holidaysevents.so,/usr/lib/qt/plugins/plasmacalendarplugins/holidaysevents.so</default>
		</entry>
	</group>


